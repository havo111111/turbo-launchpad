{
  "version": 2,
  "name": "turbo-launchpad",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
