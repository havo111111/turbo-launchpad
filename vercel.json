{
  "version": 2,
  "name": "turbo-launchpad",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/((?!api|_next/static|_next/image|favicon.ico).*)",
      "dest": "/index.html"
    }
  ]
}
